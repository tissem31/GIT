<config>
  <native xmlns="http://cisco.com/ns/yang/Cisco-IOS-XE-native">
    <pseudowire-class>
        <name>{{ pseudowire["name"] }}</name>
        <encapsulation xmlns="http://cisco.com/ns/yang/Cisco-IOS-XE-l2vpn">{{ pseudowire["encapsulation"] }}</encapsulation>
    </pseudowire-class>
    <interface>
      <GigabitEthernet>
        <name>{{ pseudowire["interface"] }}</name>
        <xconnect xmlns="http://cisco.com/ns/yang/Cisco-IOS-XE-l2vpn">
          <address>{{ pseudowire["ip-address"] }}</address>
          <vcid>{{ pseudowire["vcid"] }}</vcid>
          <encapsulation>{{ pseudowire["encapsulation"] }}</encapsulation>
          <pw-class>{{ pseudowire["name"] }}</pw-class>
        </xconnect>
      </GigabitEthernet>
    </interface>
  </native>
</config>